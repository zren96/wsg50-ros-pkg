<?xml version="1.0"?>
<launch>

	<arg name="remap" default="true" /><!-- Remap joint_states topic. e.g. for joint_state_publisher -->

  	<remap from="/joint_states" to="/joint_states_gripper" if="$(arg remap)"/>
	<node name="wsg50_driver"  pkg="wsg_50_driver" type="wsg_50_ip" output="screen" >
		<param name="ip" type="string" value="192.168.1.20"/>
		<param name="port" type="int" value="1000"/>
        <param name="reconnect_timeout_ms" type="int" value="250"/>
		<rosparam command="load" file="$(find wsg_50_driver)/launch/grippercontroller.yaml" />
	</node>

   <!--  <node name="wsg50_ack_error"  pkg="wsg_50_driver" type="wsg50_acknowledge_gripper_error_node.py" output="screen" >
    </node>
 -->
</launch>

